{
  "name": "Control",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'sha256-9QAk0OxA8HJaKEorlt7qak7H6/PLf3jEAoK9ue94wwg='; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://www.duolingo.com/*"
      ],
      "js": [
        "content.js",
        "options.js",
        "jquery.js"
      ],
      "all_frames": false
    }
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "index.html"
  }
}
